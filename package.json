    "engines":{
        "node":"22.17.0"
    },
    "author": "Start Bootstrap",
    "contributors": [
        {
            "name": "David Miller",
            "url": "https://davidmiller.io/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StartBootstrap/startbootstrap-clean-blog.git"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "bootstrap": "5.2.3",
        "connect-flash": "^0.1.1",
        "ejs": "^3.1.10",
        "express": "^5.1.0",
        "express-fileupload": "^1.5.2",
        "express-session": "^1.18.2",
        "mongodb": "^6.20.0",
        "mongoose": "^7.8.7",
        "mongoose-unique-validator": "^4.0.1",
        "nodemon": "^3.1.10"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "2.29.1",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "8.4.21",
        "prettier": "2.8.6",
        "pug": "3.0.2",
        "sass": "1.60.0",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    },
    "main": "index.js"
